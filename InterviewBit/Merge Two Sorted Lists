# Definition for singly-linked list.
# class ListNode:
#    def __init__(self, x):
#        self.val = x
#        self.next = None

class Solution:
    # @param A : head node of linked list
    # @param B : head node of linked list
    # @return the head node in the linked list
    def mergeTwoLists(self, A, B):
        a=[]
        cur=A
        curr=B
        while cur:
            a.append(cur.val)
            cur=cur.next
        while curr:
            a.append(curr.val)
            curr=curr.next
        a.sort()
        a = [str(x) for x in a]
        
        print  (' -> '.join(a),end=' ')
 
            
       
